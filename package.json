{
  "name": "funny-bundler",
  "type": "module",
  "version": "1.0.0",
  "description": "for test bundlers",
  "private": true,
  "scripts": {
    "start": "npm run clean && tsx .node",
    "build:webpack": "webpack",
    "build:rollup": "rollup --config",
    "build:esbuild": "node esbuild.run.js",
    "clean": "rm -rf rollup_dist webpack_dist esbuild_dist && npm cache clean --force && npm cache verify"
